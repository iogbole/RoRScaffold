<h1>Listing jedis</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Sex</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jedis.each do |jedi| %>
      <tr>
        <td><%= jedi.first_name %></td>
        <td><%= jedi.last_name %></td>
        <td><%= jedi.sex %></td>
        <td><%= jedi.location %></td>
        <td><%= link_to 'Show', jedi %></td>
        <td><%= link_to 'Edit', edit_jedi_path(jedi) %></td>
        <td><%= link_to 'Destroy', jedi, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jedi', new_jedi_path %>
